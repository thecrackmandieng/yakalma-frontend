{
  "name": "yakalma-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build --configuration production --aot --build-optimizer --vendor-chunk=true --common-chunk=true --named-chunks=false --output-hashing=none && ng run yakalma-frontend:server:production",
    "build:ssr:prod": "ng build --configuration production --aot --build-optimizer --vendor-chunk=true --common-chunk=true --named-chunks=false --output-hashing=none && ng run yakalma-frontend:server:production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr": "node --max-old-space-size=384 dist/yakalma-frontend/server/server.mjs",
    "serve:ssr:yakalma-frontend": "node --max-old-space-size=384 dist/yakalma-frontend/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/google-maps": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.11",
    "@fortawesome/angular-fontawesome": "^0.12.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@tailwindcss/postcss": "^4.1.11",
    "chart.js": "^4.4.9",
    "express": "^4.18.2",
    "leaflet": "^1.9.4",
    "ng2-charts": "^3.0.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.20",
    "@angular/cli": "^18.0.7",
    "@angular/compiler-cli": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2"
